{
    "externalTranspiler": {
        "name": "babel",
        "options": {}
    },
    "compilerOptions": {
        "target": "ES6",
        "jsx": "react",
        "module": "es6",
        "emitDecoratorMetadata": true,
        "removeComments": false,
        "sourceMap": false,
        "inlineSourceMap": true,
        "noEmitHelpers": true
    },
    "exclude": [
        "node_modules",
        "dist"
    ]
}